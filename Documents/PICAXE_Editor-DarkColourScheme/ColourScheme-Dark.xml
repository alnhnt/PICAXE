		<Triggers>
			<AutoReplaceTrigger Key="AutoReplaceTrigger">
				<AutoReplaceTriggerActivators>
					<AutoReplaceTriggerActivator Character="	" />
					<AutoReplaceTriggerActivator Character="&#xD;" />
					<AutoReplaceTriggerActivator Character=" " />
					<AutoReplaceTriggerActivator Character="$" />
					<AutoReplaceTriggerActivator Character="%" />
